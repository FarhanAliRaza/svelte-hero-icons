{
  "name": "svelte-hero-icons",
  "version": "4.0.3",
  "description": "Heroicons for Svelte (Project based on heroicons)",
  "repository": {
    "type": "git",
    "url": "https://github.com/JustinVoitel/svelte-hero-icons"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "author": "Justin Voitel <mail@justinv.me>",
  "license": "MIT",
  "keywords": [
    "svelte",
    "component",
    "icon",
    "icons",
    "heroicons",
    "tailwindcss"
  ],
  "bugs": {
    "url": "https://github.com/JustinVoitel/svelte-hero-icons/issues"
  },
  "homepage": "https://github.com/JustinVoitel/svelte-hero-icons#readme",
  "files": [
    "heroicons",
    "*.svelte",
    "*.js",
    "*.d.ts"
  ],
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "build:icons": "pnpm run clean && node --loader ts-node/esm build.ts",
    "clean": "rm -rf src/lib/heroicons src/lib/index.js",
    "release:beta": "pnpm run package && cd package && pnpm publish --tag beta",
    "release": "pnpm run package && cd package && pnpm publish"
  },
  "devDependencies": {
    "@sveltejs/adapter-node": "next",
    "@sveltejs/adapter-vercel": "next",
    "@sveltejs/kit": "next",
    "@types/node": "^16.11.20",
    "heroicons": "^1.0.5",
    "html-parse-stringify": "^3.0.1",
    "pascalcase": "^1.0.0",
    "svelte": "^3.46.2",
    "svelte-check": "^2.3.0",
    "svelte-hero-icons": "*",
    "svelte-preprocess": "^4.10.2",
    "svelte2tsx": "^0.4.14",
    "ts-node": "^10.4.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4"
  },
  "type": "module",
  "main": "index.js"
}